<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">



    <!-- 消费者 应用信息 -->
    <dubbo:application name="userCenter"/>
    <!-- 注册中心服务地址 -->
    <dubbo:registry protocol="zookeeper" address="47.93.230.61:2181"/>
    <dubbo:protocol  name="dubbo" port="-1"/>

    <bean id="userResource" class="resourceImpl.UserResourceImpl"/>
    <dubbo:service interface="resource.UserResource" ref="userResource" timeout="600000"/>

    <bean id="JpushResource" class="resourceImpl.JpushResourceImpl"/>
    <dubbo:service interface="resource.JpushResource" ref="JpushResource" timeout="600000"/>

    <bean id="UserInfoResouce" class="resourceImpl.UserInfoResouceImpl"/>
    <dubbo:service interface="resource.UserInfoResouce" ref="JpushResource" timeout="600000"/>

    <bean id="VerifyCodeResource" class="resourceImpl.VerifyCodeResourceImpl"/>
    <dubbo:service interface="resource.VerifyCodeResource" ref="JpushResource" timeout="600000"/>

</beans>